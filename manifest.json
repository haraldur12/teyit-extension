{
  "manifest_version": 2,

  "name": "Teyit.Link(er)",
  "description": "Gets the current page and archives it on teyit.link",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
        "tabs",
        "storage"
    ]
}
